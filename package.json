{
  "name": "oscrouter",
  "version": "1.0.0",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch src/"
  },
  "main": "dist/index.js",
  "nodemonConfig": {
    "exec": "babel-node test.js",
    "verbose": true,
    "delay": 2000,
    "ext": "*.js"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "async": "^3.1.0",
    "body-parser": "^1.20.1",
    "osc": "^2.3.1",
    "route-parser": "0.0.5",
    "sha256": "^0.2.0",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "grunt-babel": "^8.0.0",
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "grunt": "^1.0.4",
    "grunt-contrib-copy": "^1.0.0",
    "load-grunt-tasks": "^5.1.0"
  }
}
